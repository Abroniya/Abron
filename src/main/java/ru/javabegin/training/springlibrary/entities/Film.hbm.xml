<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ru.javabegin.training.springlibrary.entities.Film" table="film" schema="" catalog="komp">
        <id name="kodFilm" column="kod_film"/>
        <property name="kodVid" column="kod_vid"/>
        <property name="film" column="film"/>
        <property name="manufacturer" column="manufacturer"/>
        <property name="specification" column="Specification"/>
    </class>
</hibernate-mapping>